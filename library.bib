   journal = {ZooKeys},
   keywords = {Inventory,Natural history,Open formations,Restinga,Serpentes,Species richness},
   pages = {107-125},
   publisher = {Pensoft Publishers},
   title = {Ecological diversity of a snake assemblage from the atlantic forest at the south coast of paraíba, northeast Brazil},
   volume = {2018},
   year = {2018}
}
