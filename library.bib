@article{Couto2024,
   abstract = {<p>The reproductive potential of an individual is crucial for ensuring the production of offspring. However, knowledge about polyandry in anurans is limited. In this report, we present the first documented case of polyandrous reproduction or multiple reproduction in Phyllomedusa tetraploidea (Anura: Hylidae) Pombal &amp; Haddad, 1992. This observation occurred during a rainy night in the Atlantic Forest region of northwest Paraná, Brazil. The multiple reproduction event involved four males (three of which were identified as satellites) and one female. Notable behaviors were recorded, including a vocalizing male assisting the female in folding leaves with his feet before depositing the eggs. The entire multiple reproduction event lasted about 40 minutes. Additionally, we observed a pair of P. tetraploidea engaging in axillary amplexus, where the male and female positioned their bellies toward each other. This amplexus lasted approximately 80 minutes, from their initial meeting until the eggs were deposited and they separated. This study contributes to our understanding of P. tetraploidea's reproductive mode, detailing behaviors from vocalization to egg deposition. It also adds to our knowledge of the evolution of reproductive strategies.</p>},
   author = {Rodney Murillo Peixoto Couto and Maiara Cabrera Miguel and Anderson Correa Branco and Priscilla Soares dos Santos and Douglas Alves Lopes and Daniel Montanher Polizel and Rosemeire da Silva Filardi and Antonio Carlos de Laurentiz},
   doi = {10.55905/cuadv16n6-105},
   issn = {1989-4155},
   issue = {6},
   journal = {Cuadernos de Educación y Desarrollo},
   month = {6},
   pages = {e4522},
   title = {Reproductive behavior of Phyllomedusa tetraploidea (Anura: Hylidae) Pombal \&amp; Haddad, 1992 in the Brazilian Atlantic Forest},
   volume = {16},
   year = {2024}
}
